@{
    string userAccount = "";
    if (Session[Band_Web.Models.SessionDictionary.User_Account] != null)
    {
        userAccount = Session[Band_Web.Models.SessionDictionary.User_Account].ToString();
        <li class="nav-item">
            <div class="background-one link-container">
                <a class="link-one" href="/user/UserEdit" id="signUp">Welcome! @userAccount</a>
            </div>
        </li>
        <li class="nav-item">
            <div class="background-one link-container">
                <a class="link-one" id="login" href="#">LOGOUT</a>
            </div>
        </li>
    }
    else
    {
        userAccount = "SIGN UP";
        <li class="nav-item">
            <div class="background-one link-container">
                <a class="link-one" href="/signUpPage/SignUp">@userAccount</a>
            </div>
        </li>
        <li class="nav-item">
            <div class="background-one link-container">
                <a class="link-one" id="login" href="/loginPage/loginPage">LOGIN</a>
            </div>
        </li>
    }
}